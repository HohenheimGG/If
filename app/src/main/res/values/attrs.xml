<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--定制化组件属性-->
    <!--IconView属性-->
    <declare-styleable name="IconView">
        <attr name="icon_icon" format="reference"/>
        <attr name="icon_focus_color" format="color"/>
        <attr name="icon_unfocus_color" format="color"/>
    </declare-styleable>

    <!--IFToolBar属性-->
    <declare-styleable name="IFToolBar">
        <attr name="start_color" format="color"/>
        <attr name="end_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="TabIndicator">
        <attr name="focus_color" format="color"/>
        <attr name="unfocus_color" format="color"/>
    </declare-styleable>

</resources>